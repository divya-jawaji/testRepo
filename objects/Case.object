    <fields>
        <fullName>Closure_Days__c</fullName>
        <externalId>false</externalId>
        <formula>IF(ISPICKVAL(Status,&quot;Closed&quot;),(ClosedDate-CreatedDate),0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
