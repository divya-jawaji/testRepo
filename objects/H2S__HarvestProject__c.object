        <fullName>H2S__Burn__c</fullName>
        <deprecated>false</deprecated>
        <description>100 is the total max width in pixels; 15 is height in pixels.
/s.gif</description>
        <externalId>false</externalId>
        <formula>if(or( ISBLANK( H2S__TotalProjectHours__c ) ,H2S__TotalProjectHours__c=0),&quot;No Budget&quot;,
if(H2S__TotalProjectHours__c&gt;0,if( H2S__TotalTime__c  /  H2S__TotalProjectHours__c  &gt; 1, IMAGE(&quot;/img/samples/color_red.gif&quot;, &quot;red&quot;, 15, 100) ,
IMAGE(&quot;/img/samples/color_green.gif&quot;, &quot;green&quot;, 15, (H2S__TotalTime__c / H2S__TotalProjectHours__c ) * 100) &amp; IMAGE(&quot;/img/samples/color_yellow.gif&quot;, &quot;yellow&quot;, 15, 100 - ((H2S__TotalTime__c / H2S__TotalProjectHours__c) * 100))),&quot;&quot;)&amp;&quot; &quot;&amp;text( round(H2S__BurnPercent__c *100,0))&amp;&quot;%&quot;)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
